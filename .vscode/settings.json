{
    "[latex]": {
        "editor.tabSize": 2
    },
    "[latex-expl3]": {
        "editor.tabSize": 2
    },
    "[lua]": {
        "editor.tabSize": 2
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        ".busted": "lua",
        "*.aux": "latex",
        "*.bbl": "latex",
        "*.lvt": "latex",
        "*.rockspec": "lua",
        "*.sty": "latex-expl3",
        "*.tlg": "latex-expl3",
        "*_*.txt": "xml",
        "DEPENDS.txt": "shellscript",
        "citeproc-test-skip.txt": "shellscript"
    },
    "latex-workshop.intellisense.update.aggressive.enabled": true,
    "Lua.runtime.version": "Lua 5.3"
}
